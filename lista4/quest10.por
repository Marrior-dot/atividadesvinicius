programa {
	funcao inicio() {
		inteiro N = 0
		inteiro um = 1
		
		escreva("Digite um valor: ")
		leia(N)
		
        escreva(um+"\n")
		se (N > 0){
		    enquanto (um < valor){
		        um += 1
		        escreva(um+"\n")
		    }
		}
		se (N < 0){
		    enquanto(um > valor){
		        um -= 1
                escreva(um+"\n")		        
		    }
		    
		}
	}
}
